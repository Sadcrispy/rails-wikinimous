<div class="container">
  <ul class="container list-unstyled">
    <% @articles.each do |article| %>
      <div class="card m-2 p-2">
      <li><h5><%= link_to article.title, article_path(article), class: "text-decoration-none text-danger" %></h5></li>
      <li><%= link_to article.content, article_path(article), class: "text-decoration-none text-danger" %></li>
      </div>
    <% end %>
  </ul>
   <div class="container">
    <%= link_to "POST", new_article_path, class: "text-decoration-none text-danger" %>
  </div>
</div>
